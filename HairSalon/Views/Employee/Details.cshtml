@{
  Layout = "_Layout";
}

<h3>Current Clients:</h3>
@foreach (var client in @Model["client"])
{
  <ul>
    <li>@Model.GetClientName()</li>
  </ul>
}
  <br>
      <div class="container">
        <div class="col-md-6">
          <form action="/employees/@Model.GetEmployeeId()/newClient" method="post">
            <h3>Add A New Client</h3>
            <div class="form-group">
              <label for="clientName">Name</label>
              <input class="form-control" name="clientName" type="text" placeholder="First Last">
            </div>
            <button type="submit" class="btn btn-info">Add</button>
          </form>
        </div>
      </div>
